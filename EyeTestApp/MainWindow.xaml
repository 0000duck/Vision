<Window x:Class="EyeTestApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EyeTestApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" WindowStyle="None" Background="Transparent" AllowsTransparency="True">
    <Grid>
        <Canvas x:Name="Canvas">
            <Grid ClipToBounds="False" x:Name="Cursor">
                <Ellipse Width="48" Height="48" StrokeThickness="4" Stroke="White" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="-100"/>
                <Ellipse Fill="White" Width="6" Height="6" Margin="-100"/>
            </Grid>
        </Canvas>
    </Grid>
</Window>
